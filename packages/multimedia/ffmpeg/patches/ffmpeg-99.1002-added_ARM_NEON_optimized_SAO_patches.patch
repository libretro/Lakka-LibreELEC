+        ldr    r5, [sp, #24] // offset_table
+        vpush {d8-d15}
+        vld1.8  {q0, q1}, [r5] // offset table
+
+1:      subs    r4, #1
+        vld1.8  {q12}, [r1], r3
+
+        vshr.u8   q8, q12, #3
+
+        vtbl.8  d16, {q0, q1}, d16
