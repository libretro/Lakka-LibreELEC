+
+function ff_hevc_sao_band_w64_neon_8, export=1
+        push  {r4-r8}
+        ldr    r4, [sp, #20] // height
+        ldr    r5, [sp, #24] // offset_table
+        vpush {d8-d15}
+        vld1.8  {q0, q1}, [r5] // offset table
+
+1:      subs    r4, #1
+        vld1.8  {q12-q13}, [r1]!
+        vld1.8  {q14-q15}, [r1], r3
+        sub     r1, #32
+
+        vshr.u8   q8, q12, #3
+        vshr.u8   q9, q13, #3
+        vshr.u8  q10, q14, #3
+        vshr.u8  q11, q15, #3
+
+        vtbl.8  d16, {q0, q1}, d16
+        vtbl.8  d17, {q0, q1}, d17
+        vtbl.8  d18, {q0, q1}, d18
+        vtbl.8  d19, {q0, q1}, d19
