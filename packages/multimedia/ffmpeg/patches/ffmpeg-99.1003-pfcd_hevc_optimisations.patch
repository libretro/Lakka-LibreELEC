+// mc_setup(y_x, ref_y_base, y2_x2, ref_y2_base, frame_width_height, pitch, dst_pitch, offset_shift, next_kernel)
+static void rpi_simulate_inter_luma(HEVCContext *s,uint32_t *p)
+{
+  uint32_t next_kernel;
+  int y_x,y2_x2;
+  uint32_t x0;
+  uint32_t y0;
+  uint32_t x2;
+  uint32_t y2;
