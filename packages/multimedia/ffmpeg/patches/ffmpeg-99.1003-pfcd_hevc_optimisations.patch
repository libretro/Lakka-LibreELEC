+unsigned vpu_execute_code( unsigned code, unsigned r0, unsigned r1, unsigned r2, unsigned r3, unsigned r4, unsigned r5)
+{
+  unsigned r;
+  gpu_lock();
+  r = execute_code(gpu->mb, code, r0, r1, r2, r3, r4, r5);
+  gpu_unlock();
+  return r;
+}
