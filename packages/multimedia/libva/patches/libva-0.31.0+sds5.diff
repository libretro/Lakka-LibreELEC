+static VAStatus va_compat_BeginPicture (
+    VADriverContextP ctx,
+    VAContextID context,
+    VASurfaceID render_target
+)
+{
+    VACompatContextP compat = ctx->compat;
+    compat->skip_frame = 0;
+    return VA_INVOKE_COMPAT(vaBeginPicture, ctx, context, render_target);
+}
