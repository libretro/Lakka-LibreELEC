+	int fd;
+
+	/* Some stats, for information only */
+	struct {
+		uint64_t runt_packets;
+		uint64_t incoming_dsi;
+		uint64_t rx_bytes;
+		uint64_t tx_bytes;
+		uint64_t requests_pending;
+	} stats;
+
+	/* General information */
