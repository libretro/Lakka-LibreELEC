+
+	/* UAMs */
+	unsigned int supported_uams;
+	unsigned int using_uam;
+
+	/* Authentication */
+	char username[AFP_MAX_USERNAME_LEN];
+	char password[AFP_MAX_PASSWORD_LEN];
+
