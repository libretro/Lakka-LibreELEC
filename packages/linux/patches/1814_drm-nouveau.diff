+	struct drm_device *dev = pci_get_drvdata(pdev);
+	(void)dev;
+	return -ENODEV;
+}
+
+static int
+nouveau_pci_resume(struct pci_dev *pdev)
+{
+	struct drm_device *dev = pci_get_drvdata(pdev);
+	(void)dev;
+	return -ENODEV;
+}
