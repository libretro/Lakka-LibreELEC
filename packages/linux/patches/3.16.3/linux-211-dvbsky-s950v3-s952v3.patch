+	dvbsky_m88rs6000_writereg(state, 0x56, tmp);
+
+	tmp = dvbsky_m88rs6000_readreg(state, 0x76);
+	tmp &= ~0x80;
+	dvbsky_m88rs6000_writereg(state, 0x76, tmp);
+
+	return 0;
