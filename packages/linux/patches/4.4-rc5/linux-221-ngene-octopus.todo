+static enum dvbfe_search search(struct dvb_frontend *fe)
+{
+	int r;
+	u32 loops = 20, i;
+	fe_status_t status;
+
+	r = set_parameters(fe);
+
+	for (i = 0; i < loops; i++)  {
+		msleep(50);
+		r = read_status(fe, &status);
+		if (r)
+			return DVBFE_ALGO_SEARCH_ERROR;
+		if (status & FE_HAS_LOCK)
+			break;
+	}
+
+	if (status & FE_HAS_LOCK)
+		return DVBFE_ALGO_SEARCH_SUCCESS;
+	else
+		return DVBFE_ALGO_SEARCH_AGAIN;
