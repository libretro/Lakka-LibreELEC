+		}
+
+		state->Regs[IRQ_CLEAR] |= 0x80; /* Reset IRQ */
+		CHK_ERROR(update_reg(state, IRQ_CLEAR));
+
+		CHK_ERROR(PowerOn(state));
+
+		state->Regs[RF_FREQUENCY_1] = ((f >> 16) & 0xFF);
+		state->Regs[RF_FREQUENCY_2] = ((f >>  8) & 0xFF);
