+#include "si2157.h"
+
+#define TBS5220_READ_MSG 0
+#define TBS5220_WRITE_MSG 1
+
+#define TBS5220_RC_QUERY (0x1a00)
+
+struct tbs5220_state {
+	struct i2c_client *i2c_client_demod;
+	struct i2c_client *i2c_client_tuner; 
+	u32 last_key_pressed;
+};
