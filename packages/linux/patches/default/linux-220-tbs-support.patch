+static int avl6882_demod_lock_wait(struct avl6882_priv *priv, u8 *lock_flag)
+{
+	int ret, retry = 50;
+	enum fe_status locked;
+	do {
+		ret = avl6882_read_status(&priv->frontend, &locked);
+		if (ret) {
+			*lock_flag = 0;
+			break;
+		}
+		if (locked) {
+			*lock_flag = 1;
+			break;
+		}
+		msleep(20);
+	} while (--retry);
+	return ret;
+}
