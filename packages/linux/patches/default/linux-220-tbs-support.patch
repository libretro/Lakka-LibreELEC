+	struct tbsecp3_i2c *i2c_adap = i2c_get_adapdata(i2c);
+	struct tbsecp3_dev *dev = i2c_adap->dev;
+	u32 val ,reg;
+	
+	reg = 0xc-rf_in*4;
+	
+	val = tbs_read(TBSECP3_GPIO_BASE, reg);
+	if(flag)
+		val |= 2;
+	else
+		val &= ~2;
+		
+	tbs_write(TBSECP3_GPIO_BASE, reg, val);
