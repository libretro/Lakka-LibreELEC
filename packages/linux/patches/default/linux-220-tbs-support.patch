+		R848_Sys_Info.BW=0x40;							//BW=7M   R848:R19[6:5]
+		R848_Sys_Info.FILT_CAL_IF=8025; 		 //CAL IF	
+		R848_Sys_Info.HPF_COR=0x03; 			 //R19[3:0]=3 //3
