+{
+	if (0 == dvb_usb_device_init(intf, &tbs5881_properties,
+			THIS_MODULE, NULL, adapter_nr)) {
+		return 0;
+	}
+	return -ENODEV;
+}
+
+static struct usb_driver tbs5881_driver = {
+	.name = "tbs5881",
