+    r |= AVL6882_WR_REG8(priv, 0xa00 + rc_DVBTx_l1_proc_only_caddr_offset, 1);
+    r |= avl6882_exec_n_wait(priv,AVL_FW_CMD_ACQUIRE);
+
+    return r;
