+	RTV_BW_MODE_8MHZ, /* DVB_T, FULLSEG */
+	RTV_BW_MODE_430KHZ, /* 1SEG at 6MHz BW */
+	RTV_BW_MODE_500KHZ, /* 1SEG at 7MHz BW */
+	RTV_BW_MODE_571KHZ, /* 1SEG at 8MHz BW */
