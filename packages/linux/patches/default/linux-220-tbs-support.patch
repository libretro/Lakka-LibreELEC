+static int set_tone(struct dvb_frontend *fe, enum fe_sec_tone_mode tone)
+{
+	struct mxl *state = fe->demodulator_priv;
+
+	switch (mode) {
+	case 1:
+		set_input_tone(fe, tone, state->rf_in);
+		break;
+	case 0:
+	default:
+		if (tone == SEC_TONE_ON)
+			state->rf_in &= ~1;
+		else
+			state->rf_in |= 1;
+		break;
+	}
+
+	return 0;
+}
