+{
+	u32	c;
+	s32	ret;
+	s32	logtbl[] = {
+	0	,-1000,-699	,-523,-398,
+	-301,-222 ,-155	,-97 ,-46 ,
+	0	,41	,79	,114	,146	,
+	176	,204	,230	,255	,
+	279	,301	,322	,342	,
+	362	,380	,398	,415	,
+	431	,447	,462	,477	,
+	491	,505	,519	,531	,
+	544	,556	,568	,580	,
+	591	,602	,613	,623	,
+	633	,643	,653	,663	,
+	672	,681	,690	,699	,
+	708	,716	,724	,732	,
+	740	,748	,756	,763	,
+	771	,778	,785	,792	,
+	799	,806	,813	,820	,
+	826	,833	,839	,845	,
+	851	,857	,863	,869	,
+	875	,881	,886	,892	,
+	898	,903	,908	,914	,
+	919	,924	,929	,934	,
+	940	,944	,949	,954	,
+	959	,964	,968	,973	,
+	978	,982	,987	,991	,
+	996	,1000};
+	c = 0;
+
+	while( cnr > 100 ){
+		cnr = cnr / 10;
+		c++;
+	}
+	ret = logtbl[cnr] + c*1000 + 1000;
+
+	return ret;
+}
