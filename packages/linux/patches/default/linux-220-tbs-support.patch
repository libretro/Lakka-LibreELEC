+	u32 freq = 0;
+	u8 tmp;
+	
+	pr_info("delivery_system=%d frequency=%d\n",
+			c->delivery_system, c->frequency);
+	if (!dev->active) {
+		ret = -EAGAIN;
+		goto err;
+	}
+	MXL603_REG_CTRL_INFO_T MxL603_DigitalDvbc[] = 
+	{
+	  {0x0C, 0xFF, 0x00},
+	  {0x13, 0xFF, 0x04},
+	  {0x53, 0xFF, 0x7E},
+	  {0x57, 0xFF, 0x91},
+	  {0x5C, 0xFF, 0xB1},
