+		R848_Sys_Info.FILT_CAL_IF=9000; 		  //CAL IF //9150
+		R848_Sys_Info.HPF_COR=0x0A; 			 //R19[3:0]=10
+		R848_Sys_Info.FILT_EXT_ENA=0x10;	  //R19[4]=1, ext enable
