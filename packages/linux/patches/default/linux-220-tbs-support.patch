+#define BUILD_HYDRA_CMD(cmdID, reqType, size, dataPtr, cmdBuff)		\
+	do {								\
+		cmdBuff[0] = ((reqType == MXL_CMD_WRITE) ? MXL_HYDRA_PLID_CMD_WRITE : MXL_HYDRA_PLID_CMD_READ); \
