+static u32 get_big_endian(u8 numOfBits, const u8 buf[])
+{
+	u32 retValue = 0;
+
+	switch (numOfBits) {
+	case 24:
+		retValue = (((u32) buf[0]) << 16) |
+			(((u32) buf[1]) << 8) | buf[2];
+		break;
+	case 32:
+		retValue = (((u32) buf[0]) << 24) |
+			(((u32) buf[1]) << 16) |
+			(((u32) buf[2]) << 8) | buf[3];
+		break;
+	default:
+		break;
+	}
+
+	return retValue;
+}
+
+static void flip_data_in_dword(u32 size, u8 *d)
+{
+	u32 i;
+	u8 t;
+
+	for (i = 0; i < size; i += 4) {
+		t = d[i + 3]; d[i + 3] = d[i]; d[i] = t;
+		t = d[i + 2]; d[i + 2] = d[i + 1]; d[i + 1] = t;
+	}
+}
