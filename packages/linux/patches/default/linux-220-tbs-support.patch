+		cmdBuff[3] = cmdID;					\
+		cmdBuff[4] = 0x00;					\
+		cmdBuff[5] = 0x00;					\
+		convert_endian(MXL_ENABLE_BIG_ENDIAN, size, (u8 *)dataPtr); \
+		memcpy((void *)&cmdBuff[6], dataPtr, size);		\
+	} while(0) //;
