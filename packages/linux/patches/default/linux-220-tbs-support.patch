+static int tbs5927_tuner_attach(struct dvb_usb_adapter *adap)
+{
+	if (!dvb_attach(stv6120_attach, adap->fe_adap->fe,
+		      &adap->dev->i2c_adap, &tbs5927_stv6120_cfg, 0))
+		return -EIO;
+
+	return 0;
+}
