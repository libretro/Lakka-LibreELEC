+		R848_Sys_Info.FILT_EXT_ENA=0x10;	  //R19[4]=1, ext enable
+		R848_Sys_Info.FILT_EXT_WIDEST=0x00;//R38[2]=0, ext normal
+		R848_Sys_Info.FILT_EXT_POINT=0x03;	 //R38[1:0]=11, buf 8
+		break;
