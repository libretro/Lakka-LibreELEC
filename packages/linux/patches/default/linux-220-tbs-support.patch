+		R848_Sys_Info.FILT_CAL_IF=10800;		 //CAL IF
+		R848_Sys_Info.HPF_COR=0x0C; 			 //R19[3:0]=12
+		R848_Sys_Info.FILT_EXT_ENA=0x10;	  //R19[4]=1, ext enable
