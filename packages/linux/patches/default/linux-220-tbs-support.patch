+			d->fe_adap->fe->ops.set_voltage = tbsqboxs1_set_voltage;
+
+			buf[0] = 7;
+			buf[1] = 1;
+			tbsqboxs1_op_rw(u->udev, 0x8a, 0, 0,
+					 buf, 2, TBSQBOX_WRITE_MSG);
+
+			strlcpy(d->fe_adap->fe->ops.info.name,u->props.devices[0].name,52);
+			return 0;
+		}
+	}
+
+	return -EIO;
+}
