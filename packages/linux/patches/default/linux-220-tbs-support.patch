+		/* request 0x82 = i2c write (data=buf, len=64) -- no i2c stop? -- */
+
+		/* request 0x90 = i2c read n bytes (data=[rd_len, slave_addr, x?]; len=2+x?) */
+		/* request 0x91 = i2c grab received data (data=buf, len=rd_len) */
+
