--- libiconv-20130504/build-aux/snippet/warn-on-use.h	1970-01-01 01:00:00.000000000 +0100
+++ libiconv-20130504.patch/build-aux/snippet/warn-on-use.h	2013-05-04 10:20:07.531699073 +0200
@@ -0,0 +1,109 @@
