if [ -n "${PASTEUSR}" -a "${PASTEPWD}" ]; then
  cat "$@" | curl -F 'f:1=<-' ${PASTEUSR}:${PASTEPWD}@ix.io
else
  cat "$@" | curl -F 'f:1=<-' http://ix.io
fi
