+        iocache.s[0].addr = 0;
+        iocache.s[0].size  = 0;
+
+        iocache.s[1].cmd = 2;
+        iocache.s[1].addr = ff_hevc_buf_base(s->frame->buf[1]) + base;
+        iocache.s[1].size  = sz;
+
+        iocache.s[2].cmd = 2;
+        iocache.s[2].addr = ff_hevc_buf_base(s->frame->buf[2]) + base;
+        iocache.s[2].size  = sz;
+
+        vcsm_clean_invalid( gpu_get_mailbox(), &iocache );
