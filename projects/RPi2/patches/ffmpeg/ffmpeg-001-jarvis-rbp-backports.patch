+add r1, r1, ra30        ; mul24 r0, r1, rb14
+add r1, r1, r0
+brr.anyn -, r:yloopb
+asr r1, r1, 7          # Delay 1
+min r1, r1, rb22       # Delay 2
+max vpm, r1, 0         # Delay 3
