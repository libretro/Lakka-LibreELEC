+static void gpu_term(void)
+{
+	int mb = gpu->mb;
+	unsigned handle = gpu->vc_handle;
+  if (gpu==NULL)
+    return;
+	unmapmem((void*)gpu, sizeof(struct GPU));
+	mem_unlock(mb, handle);
+	mem_free(mb, handle);
+	qpu_enable(mb, 0);
+#ifdef RPI_USE_VCSM
+  vcsm_exit();
+#endif
+	mbox_close(mb);
+  gpu = NULL;
+}
+
+void gpu_free(GPU_MEM_PTR_T *p) {
+  int mb = gpu->mb;
+	unsigned handle = p->vc_handle;
+  gpu_lock();
+#ifdef RPI_USE_VCSM
+  if (p->vcsm_handle) {
+      mem_unlock(mb,p->vc_handle);
+      vcsm_unlock_ptr(p->arm);
+      vcsm_free(p->vcsm_handle);
+  } else {
+	unmapmem((void*)p->arm, sizeof(struct GPU));
+      mem_unlock(mb, handle);
+      mem_free(mb, handle);
+  }
+#else
+	unmapmem((void*)p->arm, sizeof(struct GPU));
+	mem_unlock(mb, handle);
+	mem_free(mb, handle);
+#endif
+
+  gpu->open_count--;
+  if (gpu->open_count==0) {
+      printf("Closing GPU\n");
+      gpu_term();
+      gpu = NULL;
+  }
+  gpu_unlock();
+}
