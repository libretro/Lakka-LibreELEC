+-mov r0, ra_x_base           # Load x
+-max r0, r0, 0; mov r1, ra_y # Load y
+-min r0, r0, rb_frame_width_minus_1 ; mov r3, ra_x2_base  # Load the frame base
+-shl ra_xshift_next, r0, 3 ; mov r2, ra_u2v_ref_offset
