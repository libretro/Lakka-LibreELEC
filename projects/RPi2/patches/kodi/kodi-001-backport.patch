+-mov r2, qpu_num
+-and r2, r2, 15
+-mov r1, r2
+-asr r1, r1, 2
+-shl r1, r1, 6
+-mov r0, r2
+-and r0, r0, 3
+-add r0, r0, r1
+-mov r1, vpm_setup(0, 4, h8p(0, 0))
+-add rb28, r0, r1
+-
