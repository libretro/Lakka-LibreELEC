+-nop                  ; mul24.ifnz r3, ra1 << 9, r1 << 9
+-sub r2, r2, r3       ; mul24    r3, ra2 << 2, r0 << 2
+-nop                  ; mul24.ifnz r3, ra2 << 10, r1 << 10
