++                break;
++            case 'C':
++                data = CHAR;
++                break;
++            case 'B':
++                data = BYTE;
++                break;
++            case 'S':
++                data = SHORT;
++                break;
++            case 'I':
++                data = INTEGER;
++                break;
++            case 'F':
++                data = FLOAT;
++                break;
++            case 'J':
++                data = LONG;
++                break;
++            case 'D':
++                data = DOUBLE;
++                break;
++            // case 'L':
++            default:
++                // stores the internal name, not the descriptor
