++            v = (t & BASE_KIND) == OBJECT || (t & DIM) != 0 ? t : TOP;
++        } else {
++            // if u is any other type, merge(u,t)=TOP whatever t
++            v = TOP;
++        }
++        if (u != v) {
++            types[index] = v;
++            return true;
++        }
++        return false;
++    }
++}
