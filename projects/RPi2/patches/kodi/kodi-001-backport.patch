++        this.longVal = bsmIndex;
++        this.strVal1 = name;
++        this.strVal2 = desc;
++        this.hashCode = 0x7FFFFFFF & (ClassWriter.INDY + bsmIndex
++                * strVal1.hashCode() * strVal2.hashCode());
++    }
