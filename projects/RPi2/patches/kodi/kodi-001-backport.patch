+-#define OPTS "vlicpPfr:ds:"
+-
+-static int
+-_qsort_str_cmp(const void *a, const void *b)
+-{
+-    const char *stra = *(char * const *)a;
+-    const char *strb = *(char * const *)b;
+-
+-    return strcmp(stra, strb);
+-}
