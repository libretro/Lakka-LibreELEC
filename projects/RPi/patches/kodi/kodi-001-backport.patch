++#else
++
+     if (!qpu_code) {
+       vpu_execute_code(p[0], p[1], p[2], p[3], p[4], p[5], p[6]);
+     } else {
+@@ -449,17 +516,29 @@ static void *vpu_start(void *arg) {
+                               0,    0   , 0   , 0   , 0   , 0   , 0); // VPU1
+ #endif
+     }
++#endif
++
+ #ifdef RPI_TIME_TOTAL_POSTED
+     end_time = Microseconds();
+     last_time = end_time;
++#ifdef RPI_COMBINE_JOBS
